{
  "name": "angular-electron-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "build": "ng build --base-href ./ && npm run electron:build && npx electron .",
    "build:prod": "ng build --configuration production --base-href ./ && npm run electron:build && npx electron .",
    "electron:build": "tsc -p tsconfig.electron.json",
    "electron:watch": "wait-on tcp:4200 && npm run electron:build && npx electron . --serve",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "lint": "eslint */**/*.ts",
    "lint:fix": "eslint */**/*.ts --fix"
  },
  "main": "main.js",
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.0",
    "@angular/common": "~11.2.0",
    "@angular/compiler": "~11.2.0",
    "@angular/core": "~11.2.0",
    "@angular/forms": "~11.2.0",
    "@angular/platform-browser": "~11.2.0",
    "@angular/platform-browser-dynamic": "~11.2.0",
    "@angular/router": "~11.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "11.0.0",
    "@angular-devkit/build-angular": "^0.900.7",
    "@angular/cli": "~11.2.0",
    "@angular/compiler-cli": "~11.2.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "12.12.6",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "electron": "11.2.0",
    "electron-builder": "22.9.1",
    "electron-reload": "1.5.0",
    "eslint": "^7.21.0",
    "eslint-config-google": "^0.14.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~8.3.0",
    "typescript": "~4.1.2",
    "wait-on": "^5.2.1"
  }
}